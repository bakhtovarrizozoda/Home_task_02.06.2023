@model AddQuoteDto
<form method="post" enctype="multipart/form-data" class="row g-3 needs-validation" novalidate>
  <div class="col-md-6">
    <label for="validationCustom01" class="form-label">Author</label>
    <input asp-for="Author" type="text" class="form-control" id="validationCustom01" value="name" required>
    <div class="valid-feedback">
      Looks good!
    </div>
  </div>
  <div class="col-md-6">
    <label for="validationCustom02" class="form-label">CategoryId</label>
    <input asp-for="CategoryId" type="text" class="form-control" id="validationCustom02" value="id" required>
    <div class="valid-feedback">
      Looks good!
    </div>
  </div>

    <div class="col-md-12">
    <label for="validationCustom02" class="form-label">Quote Text</label>
    <textarea asp-for="QuoteText" type="text" class="form-control" id="validationCustom03" value="id" required ></textarea>
    <div class="valid-feedback">
      Looks good!
    </div>
  </div>

  <div class="col-md-12">
      <input asp-for="File" type="file" class="form-control" id="inputGroupFile02">
  </div>

  <div class="col-12">
    <button  class="btn btn-primary" type="submit">Submit form</button>
  </div>
</form>